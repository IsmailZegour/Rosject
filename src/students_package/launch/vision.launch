<launch>
    <!-- NAVIGATION -->
        <!-- gmapping -->
        <include file="$(find students_gmapping)/launch/gmapping.launch" />
        <!-- move_base -->
        <include file="$(find students_move_base_launcher)/launch/move_base.launch" />
    <!-- BOTTLE -->
        <!-- bottle service server -->
        <node 
            pkg="students_vision"
            type="bottle_service_server.py"
            name="bottle_service_server"
            output="screen">
        </node>
        <!-- bottle CV -->
        <node 
            pkg="students_vision"
            type="matcher.py"
            name="matcher">
        </node>
</launch>
